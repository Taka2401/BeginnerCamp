<div class="col-4 camp-content mt-4 ml-2">
  <%= link_to camp_item_path(camp_item), class: "link-color" do %>
    <%= attachment_image_tag(camp_item, :image, format: 'jpeg', size: "190x190", class: "rounded") %> <br />
      <div class="col-12 text-center mt-1">
        <%= camp_item.name %>
      </div>
    <%= render 'shared/camp_item_values', camp_item: camp_item %>
  <% end %>
</div>